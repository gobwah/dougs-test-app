# Application Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_PREFIX=api

# Logging
LOG_LEVEL=info

# CORS Configuration
# Comma-separated list of allowed origins, or * for all origins
CORS_ORIGIN=*

# Rate Limiting
# TTL in seconds (time window)
THROTTLE_TTL=60
# Maximum number of requests per TTL window
THROTTLE_LIMIT=100
